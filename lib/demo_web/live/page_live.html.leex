            <svg id="cheese-coney-consumption-by-neighborhood-graph" class="chart-lines" width="90%" height="92%" x="10%" y="0">
          <g id="cheese-coney-consumption-by-neighborhood-lines" class="chart-lines">
            <line x1="0%" y1="0%" x2="0%" y2="100%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
            <line x1="0%" y1="100%" x2="100%" y2="100%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
              <line x1="0%" y1="80.0%" x2="100%" y2="80.0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
              <line x1="0%" y1="60.0%" x2="100%" y2="60.0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
              <line x1="0%" y1="40.0%" x2="100%" y2="40.0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
              <line x1="0%" y1="20.0%" x2="100%" y2="20.0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
              <line x1="0%" y1="0.0%" x2="100%" y2="0.0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
            <line x1="0%" y1="0%" x2="100%" y2="0%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
            <line x1="100%" y1="0%" x2="100%" y2="100%" stroke="#0f3782" stroke-width="2px" stroke-linecap="round"></line>
          </g>
        <svg id"cheese-coney-consumption-by-neighborhood-results"="" class="chart-results" width="100%" height="96%" x="0%" y="2%">
          <g>
              <rect id="Landen" class="chart-column" width="10.0%" height="12.872192290167629%" x="5.0%" y="87.12780770983237%" rx="10" ry="10" style="transition: all 1s ease;" fill="url(#blue_gradient)">
                <animate attributeName="height" values="0;30%" dur="1s" repeatCount="freeze"></animate>
                <animate attributeName="y" values="100%;70%" dur="1s" repeatCount="freeze"></animate>
              </rect>
              <rect id="Oakley" class="chart-column" width="10.0%" height="56.77487330226687%" x="25.0%" y="43.22512669773313%" rx="10" ry="10" style="transition: all 1s ease;" fill="url(#red_gradient)">
                <animate attributeName="height" values="0;30%" dur="1s" repeatCount="freeze"></animate>
                <animate attributeName="y" values="100%;70%" dur="1s" repeatCount="freeze"></animate>
              </rect>
              <rect id="Downtown" class="chart-column" width="10.0%" height="88.40132057379877%" x="45.0%" y="11.59867942620123%" rx="10" ry="10" style="transition: all 1s ease;" fill="url(#rose_gradient)">
                <animate attributeName="height" values="0;30%" dur="1s" repeatCount="freeze"></animate>
                <animate attributeName="y" values="100%;70%" dur="1s" repeatCount="freeze"></animate>
              </rect>
              <rect id="Florence" class="chart-column" width="10.0%" height="77.51704489570719%" x="65.0%" y="22.482955104292813%" rx="10" ry="10" style="transition: all 1s ease;" fill="url(#blue_gradient)">
                <animate attributeName="height" values="0;30%" dur="1s" repeatCount="freeze"></animate>
                <animate attributeName="y" values="100%;70%" dur="1s" repeatCount="freeze"></animate>
              </rect>
              <rect id="Erlanger" class="chart-column" width="10.0%" height="75.32085467360756%" x="85.0%" y="24.679145326392444%" rx="10" ry="10" style="transition: all 1s ease;" fill="url(#red_gradient)">
                <animate attributeName="height" values="0;30%" dur="1s" repeatCount="freeze"></animate>
                <animate attributeName="y" values="100%;70%" dur="1s" repeatCount="freeze"></animate>
              </rect>
          </g>
        </svg>
            </svg>
        <defs>
            <linearGradient id="blue_gradient">
              <stop stop-color="#36D1DC" offset="0%"></stop>
              <stop stop-color="#5B86E5" offset="100%"></stop>
            </linearGradient>
            <linearGradient id="blue_gradient_line" gradientUnits="userSpaceOnUse">
              <stop stop-color="#36D1DC" offset="0%"></stop>
              <stop stop-color="#5B86E5" offset="100%"></stop>
            </linearGradient>
            <linearGradient id="red_gradient">
              <stop stop-color="#FF9486" offset="0%"></stop>
              <stop stop-color="#FF1379" offset="100%"></stop>
            </linearGradient>
            <linearGradient id="red_gradient_line" gradientUnits="userSpaceOnUse">
              <stop stop-color="#FF9486" offset="0%"></stop>
              <stop stop-color="#FF1379" offset="100%"></stop>
            </linearGradient>
            <linearGradient id="rose_gradient">
              <stop stop-color="#642B73" offset="0%"></stop>
              <stop stop-color="#C6426E" offset="100%"></stop>
            </linearGradient>
            <linearGradient id="rose_gradient_line" gradientUnits="userSpaceOnUse">
              <stop stop-color="#642B73" offset="0%"></stop>
              <stop stop-color="#C6426E" offset="100%"></stop>
            </linearGradient>
        </defs>
      </svg>
    </div>
  </div>
</header>
